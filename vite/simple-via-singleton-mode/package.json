{
  "scripts": {
    "prestart": "yarn clean && yarn install && npm-run-all prestart:*",
    "prestart:main": "cd main && yarn install",
    "prestart:vite": "cd react-demo && yarn install",
    "clean": "rm -rf **/node_modules",
    "start": "run-p start:*",
    "start:main": "cd main && yarn start",
    "start:vite": "cd react-demo && yarn dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}